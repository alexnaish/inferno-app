{
  "name": "blog",
  "version": "0.0.1",
  "description": "Blog frontend",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --prefix \"[{name}]\" --names \"JS,CSS,SERVER\" \"npm run build:js\" \"npm run build:css\" \"npm run serve\" --kill-others",
    "build:js": "rollup -c --watch",
    "build:css": "node-sass --output-style compressed -rw app/scss/index.scss dist/index_files/bundle.css",
    "serve": "browser-sync start --files \"dist/**/*\" --no-notify --server dist",
    "test": "mocha"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-plugin-inferno": "^1.3.0-beta15",
    "babel-preset-es2015": "^6.18.0",
    "browser-sync": "2.18.5",
    "chai": "3.5.0",
    "concurrently": "3.1.0",
    "eslint-plugin-inferno": "^6.8.0",
    "mocha": "3.2.0",
    "node-sass": "4.0.0",
    "rollup": "0.37.0",
    "rollup-plugin-alias": "1.2.0",
    "rollup-plugin-babel": "2.7.1",
    "rollup-plugin-commonjs": "6.0.0",
    "rollup-plugin-eslint": "3.0.0",
    "rollup-plugin-filesize": "1.0.1",
    "rollup-plugin-inject": "2.0.0",
    "rollup-plugin-node-builtins": "2.0.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "1.0.1",
    "rollup-watch": "3.1.0",
    "sinon": "1.17.6"
  },
  "dependencies": {
    "babel-runtime": "^6.20.0",
    "es6-promise": "4.0.5",
    "express": "^4.14.0",
    "history": "^4.5.0",
    "inferno": "1.0.6",
    "inferno-component": "1.0.6",
    "inferno-create-element": "1.0.6",
    "inferno-redux": "1.0.6",
    "inferno-router": "1.0.6",
    "lodash": "^4.15.0",
    "minireset.css": "0.0.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  }
}
